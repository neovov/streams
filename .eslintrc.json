{
	"env": {
		"browser": true
	},
	"extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint"],
	"root": true,
	"rules": {
		"no-constant-condition": "off",
		"no-debugger": "off",
		"no-inner-declarations": "off",
		"no-mixed-spaces-and-tabs": "off"
	}
}
